{
  "name": "ftv",
  "description": "An emote extension for Fansly which adds support for emotes from popular emote extensions.",
  "private": true,
  "version": "2.0.5",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,svelte}\""
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.2",
    "@tsconfig/svelte": "^5.0.4",
    "@wxt-dev/module-svelte": "^2.0.1",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^0.21.16",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.468.0",
    "postcss": "^8.4.49",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "^5.14.1",
    "svelte-check": "^4.1.1",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.16",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "wxt": "^0.19.22",
    "zod": "^3.24.1"
  },
  "pnpm": {
    "overrides": {
      "@sveltejs/vite-plugin-svelte@<4.0.0": ">=4.0.0",
      "svelte@<5.0.0": ">=5.0.0",
      "vite@<6.0.0": ">=6.0.0"
    }
  }
}
