{
  "name": "ftv",
  "description": "An emote extension for Fansly which adds support for emotes from popular emote extensions.",
  "private": true,
  "version": "2.0.12",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,svelte}\"",
    "release": "wxt zip && wxt zip -b firefox"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "@wxt-dev/module-svelte": "2.0.3",
    "autoprefixer": "^10.4.21",
    "bits-ui": "^1.8.0",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.511.0",
    "postcss": "^8.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.25.6",
    "svelte-check": "^4.1.6",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.5",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.7",
    "wxt": "^0.20.6",
    "zod": "^3.24.2"
  },
  "pnpm": {
    "overrides": {
      "@sveltejs/vite-plugin-svelte@<4.0.0": ">=4.0.0",
      "svelte@<5.0.0": ">=5.0.0"
    }
  }
}
