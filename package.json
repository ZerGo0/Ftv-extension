{
  "name": "ftv",
  "description": "An emote extension for Fansly which adds support for emotes from popular emote extensions.",
  "private": true,
  "version": "2.0.15",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build --mv3",
    "build:firefox": "wxt build -b firefox --mv2",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,svelte}\"",
    "release": "wxt zip && wxt zip -b firefox"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tsconfig/svelte": "^5.0.4",
    "@wxt-dev/module-svelte": "2.0.3",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^2.8.10",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.525.0",
    "postcss": "^8.5.6",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "svelte": "^5.19.5",
    "svelte-check": "^4.1.4",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "wxt": "^0.20.7",
    "zod": "^3.25.67"
  },
  "pnpm": {
    "overrides": {
      "@sveltejs/vite-plugin-svelte@<4.0.0": ">=4.0.0",
      "svelte@<5.0.0": ">=5.0.0"
    }
  }
}
